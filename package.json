{
  "name": "unfold",
  "version": "0.3.2",
  "description": "Static Site Generator",
  "keywords": [
    "static site generator",
    "static site",
    "static page generator",
    "static page"
  ],
  "author": "Eric Lathrop <eric@ericlathrop.com> (http://ericlathrop.com)",
  "license": "MIT",
  "homepage": "https://github.com/ericlathrop/unfold",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericlathrop/unfold.git"
  },
  "bugs": {
    "url": "https://github.com/ericlathrop/unfold/issues"
  },
  "main": "bin/unfold.js",
  "bin": "bin/unfold.js",
  "preferGlobal": true,
  "dependencies": {
    "handlebars": "^3.0.3",
    "merge": "^1.2.0",
    "node-sass": "^3.1.2",
    "q": "^1.0.0"
  },
  "devDependencies": {
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "mock-fs": "^2.7.0"
  },
  "scripts": {
    "mocha": "mocha bin && mocha lib",
    "jshint": "jshint bin/*.js lib/*.js",
    "test": "npm run jshint && npm run mocha"
  },
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "double",
    "unused": "strict",
    "trailing": true,
    "node": true,
    "mocha": true,
    "globalstrict": true,
    "globals": {
      "describe": false,
      "it": false
    }
  }
}
